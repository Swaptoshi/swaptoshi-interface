{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"parser": "@babel/eslint-parser",
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"requireConfigFile": false,
    	"babelOptions": {
			"babelrc": false,
			"configFile": false,
			"presets": ["@babel/preset-react"]
		}
	},
	"plugins": ["react", "jsx-a11y", "import", "jsx", "react-hooks"],
	"rules": {
		"react/prop-types": 0,
		"react-hooks/exhaustive-deps": "error"
	  }
}
